{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["DOM", "ESNext"],
    "pretty": true,
    "strict": true,
    "allowJs": true,
    "alwaysStrict": true,
    "skipLibCheck": true,
    "importHelpers": false,
    "strictNullChecks": true
  },
  "exclude": ["node_modules"]
}
